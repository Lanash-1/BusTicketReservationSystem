    <style name="TextColor">
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="TextDefault">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="TextDefault16">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="FormTitle">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="TextLink">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#0000FF</item>
    </style>

    <style name="ChatBackground">
        <item name="android:background">@color/white</item>
    </style>

    <style name="TitleTextStyle">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="IconTint">
        <item name="tint">@color/black</item>
    </style>

    <style name="LinkTextAppearance">
        <item name="android:textColor">#0000EE</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="ButtonIconColor">
        <item name="drawableTint">@color/white</item>
    </style>

    <style name="chipIcon">
        <item name="chipIconTint">@color/black</item>
    </style>

    <style name="DividerColor">
        <item name="dividerColor">#808080</item>
    </style>

    <style name="Image">
        <item name="srcCompat">@drawable/search_icon_red</item>
    </style>

    <style name="PageBackground">
        <item name="android:background">#f0f0f0</item>
    </style>

    <style name="NotFoundTextStyle">
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="HeadingText">
        <item name="android:textSize">32sp</item>
        <item name="android:textColor">@color/black</item>
        `<item name="android:textStyle">bold</item>
    </style>


    <style name="DialogFragmentAnimation">
        <item name="android:windowEnterAnimation">@anim/slide_up</item>
        <item name="android:windowExitAnimation">@anim/slide_down</item>
    </style>

    <style name="AnalyticsCardTitleStyle">
        <item name="android:textSize">32sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="AppbarBackground">
        <item name="android:background">@color/appMainColor</item>
    </style>

    <style name="RecentlyViewedSourceTextStyle">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
        <item name="drawableTint">@color/black</item>
    </style>

    <style name="SearchDialogStyle">
        <item name="drawableTint">@color/black</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="DrawableTint">
        <item name="drawableTint">@color/black</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="DrawableTintConfirmButton">
        <item name="drawableTint">@color/white</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="SearchableDialogTheme">
        <item name="android:backgroundTint">@color/white</item>
    </style>

    <style name="LargeTitleText">
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="linkTextStyle">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/appMainColor</item>
        <item name="android:clickable">true</item>
        <item name="android:background">?android:selectableItemBackground</item>
    </style>

    <style name="WelcomeScreenTitleTextStyle">
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">@color/appMainColor</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="WelcomeScreenSubtitleStyle">
        <item name="android:textSize">16sp</item>
    </style>

    <style name="radio_selected">
        <item name="backgroundTint">@color/appMainColor</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/selected_radio_button</item>
    </style>

    <style name="radio_unselected">
        <item name="android:background">@drawable/unselected_radio_button</item>
    </style>


</resources>